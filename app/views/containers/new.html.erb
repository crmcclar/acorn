<p>new container view</p>

<%= form_for :container, url: containers_path do |f| %>
	<p>
		<%= f.label :title %><br>
		<%= f.text_field :title %>
	</p>

	<p>
		<%= f.label :text %><br>
		<%= f.text_area :text %>
	</p>

	<p>
		<%= f.label :category %><br>
		<%= f.text_field :category %>
	</p>

	<p>
		<%= f.submit %>
	</p>
<% end %>