<p>
  <%= f.label :title %><br/>
  <%= f.text_field :title %>
</p>

<p>
  <%= f.file_field :photo %>
</p>

<p>
  <%= f.label :date %><br/>
  <%= f.date_select :date %>
</p>

<p>
  <%= f.label :category %><br>
  <%= f.select :category, options_for_select([['Places','Places'], ['Food','Food'], ['Music','Music'], ['Thoughts','Thoughts']]) %>
</p>

<p>
  <%= f.label :text %><br/>
  <%= f.text_area :text %>
</p>

<p>
  <%= f.submit %>
</p>
